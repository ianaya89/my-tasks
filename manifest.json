{
    "name": "Todo Chrome",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Todo application as tab default page",
    "browser_action": {
        "default_title": "Todo Chrome"
    },

    "chrome_url_overrides" : {
      "newtab": "tab.html"
    },
    
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}